/App.js",["151"],"import './App.css';\nimport React from 'react';\nimport {Route, BrowserRouter} from \"react-router-dom\";\nimport News from \"./Components/Navbar/News/News\";\nimport Music from \"./Components/Navbar/Music/Music\";\nimport Settings from \"./Components/Navbar/Settings/Settings\";\nimport Friends from \"./Components/Navbar/Friends/Friends\";\nimport DialogsContainer from \"./Components/Navbar/Dialogs/DialogsContainer\";\nimport NavbarContainer from \"./Components/Navbar/NavbarContainer\";\nimport UsersContainer from \"./Components/Navbar/Users/UsersContainer\";\nimport ProfileContainer from \"./Components/Profile/ProfileContainer\";\nimport HeaderContainer from \"./Components/Header/HeaderContainer\";\n\n\nconst App = () => {\n    return (\n        <div className='app-wrapper'>\n            <HeaderContainer/>\n            <NavbarContainer/>\n            <div className='app-wrapper-content'>\n                <Route path='/dialogs' render={() => <DialogsContainer/>}/>\n                <Route path='/profile/:userId?'\n                       render={() => <ProfileContainer/>}/>\n                <Route path='/users'\n                       render={() => <UsersContainer/>}/>\n                <Route path='/news' component={News}/>\n                <Route path='/music' component={Music}/>\n                <Route path='/settings' component={Settings}/>\n                <Route path='/friends' component={Friends}/>\n            </div>\n        </div>\n    )\n}\n\nexport default App;\n","/Users/brookly/Documents/my-first/my-app/src/reportWebVitals.js",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Header/Header.jsx",["152"],"import React from 'react';\nimport style from './Header.module.css'\nimport {NavLink} from \"react-router-dom\";\n\nconst Header = (props) => {\n    return <header className={style.header}>\n        <div>\n            <img src='https://www.citypng.com/public/uploads/preview/-41601314003cc85anibww.png'/>\n        </div>\n        <div className={style.loginBlock}>\n            {props.isAuth ?  props.login :\n            <NavLink to={`/Login`}>Login</NavLink> }\n        </div>\n    </header>\n}\nexport default Header","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Navbar.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Profile/Profile.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Profile/MyPosts/MyPosts.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Profile/MyPosts/Post/Post.jsx",["153"],"import React from 'react';\nimport style from './Post.module.css'\n\nconst Post = (props) => {\n    return <div>\n        <div className={style.item}>\n            <img src='https://i.pinimg.com/originals/8b/da/ca/8bdaca81d5ddbaeb92b61d6b5787d866.jpg'/>\n            {props.message}\n            <div>\n                <button>Like</button>\n                {props.likes}\n            </div>\n        </div>\n\n    </div>\n\n}\nexport default Post","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Dialogs/Dialogs.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/News/News.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Settings/Settings.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Music/Music.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Profile/ProfileInfo/ProfileInfo.jsx",["154","155"],"import React from 'react';\nimport style from './ProfileInfo.module.css'\nimport Preloader from \"../../Common/Preloader\";\n\nconst ProfileInfo = (props) => {\n    if(!props.profile){\n        return <Preloader/>\n    }\n    return <div>\n        <div className={style.img}>\n            <img src='https://w-dog.ru/wallpapers/15/15/562916130553730.jpg'/>\n        </div>\n        <div className={style.item}>\n            <img src={props.profile.photos.large}/>\n            <div>{props.profile.aboutMe}</div>\n            <div>{props.profile.lookingForAJobDescription}</div>\n            <div>{props.profile.fullName}</div>\n        </div>\n    </div>\n\n}\nexport default ProfileInfo","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Dialogs/DialogPeople/DialogPeople.jsx",["156"],"import React from 'react'\nimport style from './../Dialogs.module.css'\nimport {NavLink} from \"react-router-dom\";\n\nconst DialogPeople = (props) => {\n    return (\n        <div className={style.dialog}>\n            <img src='https://images-na.ssl-images-amazon.com/images/I/81YDuTWSHyL.png'/>\n            <NavLink to={'/dialogs/' + props.id} activeClassName={style.active}>{props.name}</NavLink>\n        </div>\n    )\n}\n\n\n\nexport default DialogPeople","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Dialogs/Message/Messages.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Friends/Friends.jsx",["157"],"import React from 'react'\nimport style from './Friends.module.css'\n\n\nconst Friends = (props) => {\n    return (\n        <div className>\n            Friends\n        </div>\n    )\n}\n\nexport default Friends","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Friends/FriendsBest/FriendsBest.jsx",["158"],"import React from 'react'\nimport style from './FriendsBest.module.css'\nimport {NavLink} from \"react-router-dom\";\n\n\nconst FriendsBest = (props) => {\n    return (\n        <div className={style.item}>\n            <div>\n                <img\n                    src='https://images.sftcdn.net/images/t_app-cover-l,f_auto/p/1ecfc7c3-8b2b-43d8-94f7-947c1bdb4a95/3504252309/masa-cool-wallpapers-wallpaper-hd-background-screenshot.jpg'/>\n            </div>\n            <div>\n                <NavLink to={props.name} activeClassName={style.active}>{props.name}</NavLink>\n            </div>\n        </div>\n    )\n}\n\nexport default FriendsBest","/Users/brookly/Documents/my-first/my-app/src/redux/Profile-reducer.js",["159","160"],"/Users/brookly/Documents/my-first/my-app/src/redux/Sidebar-reducer.js",[],"/Users/brookly/Documents/my-first/my-app/src/redux/Dialogs-reducer.js",[],"/Users/brookly/Documents/my-first/my-app/src/redux/Store.js",["161"],"/Users/brookly/Documents/my-first/my-app/src/redux/redux-store.js",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Profile/MyPosts/MyPostsContainer.jsx",["162"],"import React from 'react';\nimport {addPostActionCreator, updateNewPostCreator} from \"../../../redux/Profile-reducer\";\nimport MyPosts from \"./MyPosts\";\nimport {connect} from \"react-redux\";\n\nlet mapStateToProps = (state) => {\n    return {\n        posts: state.ProfilePage.PostData,\n        newPostText: state.ProfilePage.newPostText\n    }\n}\nlet mapDispatchToProps = (dispatch) => {\n    return {\n        updateNewPostText: (text) => {\n            dispatch(updateNewPostCreator(text))\n        },\n        addPost: () => {\n            dispatch(addPostActionCreator())\n        }\n    }\n}\n\nconst MyPostsContainer = connect(mapStateToProps, mapDispatchToProps)(MyPosts)\n\nexport default MyPostsContainer","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Dialogs/DialogsContainer.jsx",["163"],"import React from 'react'\nimport {addMessageActionCreator, updateNewMessageCreator} from \"../../../redux/Dialogs-reducer\";\nimport Dialogs from \"./Dialogs\";\nimport {connect} from \"react-redux\";\n\n\nlet mapStateToProps = (state) => {\n    return {\n        dialog: state.DialogsPage.DialogsData,\n        message: state.DialogsPage.messagesData,\n        newMessageText: state.DialogsPage.newMessageText\n    }\n}\nlet mapDispatchToProps = (dispatch) => {\n    return {\n        addMessage: () => {\n            dispatch(addMessageActionCreator())\n        },\n        updateMessageText: (text) => {\n            dispatch(updateNewMessageCreator(text))\n        }\n    }\n}\n\nconst DialogsContainer = connect(mapStateToProps, mapDispatchToProps)(Dialogs)\n\nexport default DialogsContainer","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/NavbarContainer.jsx",["164"],"import React from 'react';\nimport {connect} from \"react-redux\";\nimport Navbar from \"./Navbar\";\n\nlet mapStateToProps=(state)=>{\n    return{\n        friends:state.SideBar.FriendsData\n    }\n}\n\nconst NavbarContainer=connect(mapStateToProps)(Navbar)\n\n\n\n\nexport default NavbarContainer","/Users/brookly/Documents/my-first/my-app/src/redux/Users-reducer.js",["165"],"import {UsersAPI} from \"../api/api\";\n\nconst FOLLOW = 'FOLLOW'\nconst UNFOLLOW = 'UNFOLLOW'\nconst SET_USERS = 'SET-USERS'\nconst SET_CURRENT_PAGE = 'SET-CURRENT-PAGE'\nconst SET_TOTAL_USERS_COUNT = 'SET-TOTAL-USERS-COUNT'\nconst SET_FETCHING = 'FETCHING'\nconst SET_FOLLOWING_PROGRESS = 'SET_FOLLOWING_PROGRESS'\n\nlet initialState = {\n    users: [],\n    pageSize: 10,\n    totalUsersCount: 0,\n    currentPage: 1,\n    isFetching: true,\n    followingInProgress: []\n}\n\nconst UsersReducer = (state = initialState, action) => {\n\n    switch (action.type) {\n        case FOLLOW:\n            return {\n                ...state,\n                users: state.users.map(user => {\n                    if (user.id === action.userId) {\n                        return {...user, followed: true}\n                    }\n                    return user\n                })\n            }\n        case UNFOLLOW:\n            return {\n                ...state,\n                users: state.users.map(user => {\n                    if (user.id === action.userId) {\n                        return {...user, followed: false}\n                    }\n                    return user\n                })\n            }\n        case SET_USERS:\n            return {\n                ...state, users: action.users\n            }\n        case SET_CURRENT_PAGE:\n            return {\n                ...state, currentPage: action.currentPage\n            }\n        case SET_TOTAL_USERS_COUNT:\n            return {\n                ...state, totalUsersCount: action.totalUsersCount\n            }\n        case SET_FETCHING:\n            return {\n                ...state, isFetching: action.isFetching\n            }\n        case SET_FOLLOWING_PROGRESS:\n            return {\n                ...state,\n                followingInProgress: action.isFetching ?\n                    [...state.followingInProgress, action.userId]\n                    : state.followingInProgress.filter(id => id != action.userId)\n            }\n        default:\n            return state\n    }\n}\n\nexport const follow = (userId) => ({type: FOLLOW, userId})\nexport const unfollow = (userId) => ({type: UNFOLLOW, userId})\nexport const setUsers = (users) => ({type: SET_USERS, users})\nexport const setCurrentPage = (currentPage) => ({type: SET_CURRENT_PAGE, currentPage})\nexport const setTotalUsersCount = (totalUsersCount) => ({type: SET_TOTAL_USERS_COUNT, totalUsersCount})\nexport const setFetching = (isFetching) => ({type: SET_FETCHING, isFetching})\nexport const setFollowingProgress = (isFetching, userId) => ({type: SET_FOLLOWING_PROGRESS, isFetching, userId})\n\nexport const getUsersThunkCreator = (currentPage, pageSize) => {\n    return (dispatch) => {\n        dispatch(setFetching(true))\n        UsersAPI.getUsers(currentPage, pageSize)\n            .then(data => {\n                dispatch(setFetching(false))\n                dispatch(setUsers(data.items))\n                dispatch(setTotalUsersCount(data.totalCount))\n            })\n    }\n}\n\nexport const followThunkCreator = (id) => {\n    return (dispatch) => {\n        dispatch(setFollowingProgress(true, id))\n        UsersAPI.followButton(id)\n            .then(data => {\n                if (data.resultCode === 0) {\n                    dispatch(follow(id))\n                }\n                dispatch(setFollowingProgress(false, id))\n            })\n    }\n}\n\nexport const unfollowThunkCreator = (id) => {\n    return (dispatch) => {\n        dispatch(setFollowingProgress(true, id))\n        UsersAPI.unfollowButton(id)\n            .then(data => {\n                if (data.resultCode === 0) {\n                    dispatch(unfollow(id))\n                }\n                dispatch(setFollowingProgress(false, id))\n            })\n    }\n}\n\nexport default UsersReducer","/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Users/UsersContainer.jsx",[],"/Users/brookly/Documents/my-first/my-app/src/Components/Navbar/Users/Users.jsx",["166","167"],"import React from \"react\"\nimport style from \"./Users.module.css\";\nimport UserPhoto from \"../../Asets/images/user-male.png\";\nimport {NavLink} from \"react-router-dom\";\nimport {UsersAPI} from \"../../../api/api\";\n\n\nlet Users = (props) => {\n\n    let pagesCount = Math.ceil(props.totalUsersCount / props.pageSize)\n    let pages = []\n    for (let i = 1; i <= pagesCount; i++) {\n        pages.push(i)\n    }\n\n    return <div>\n        <div>\n            {pages.map(p => {\n                return <span className={props.currentPage === p && style.selectedPage}\n                             onClick={(e) => {\n                                 props.onPageChanged(p)\n                             }}>{p}</span>\n            })}\n\n        </div>\n        {\n            props.users.map(u => <div key={u.id}>\n            <span>\n                <div>\n                    <NavLink to={'/profile/' + u.id}>\n                       <img src={u.photos.small != null ? u.photos.small : UserPhoto} className={style.userPhoto}/>\n                    </NavLink>\n                </div>\n                <div>\n                    {u.followed ? <button disabled={props.followingInProgress.some(id => id === u.id)} onClick={() => {\n                           props.unfollowThunkCreator(u.id)\n                        }}>Unfollow</button> :\n                        <button disabled={props.followingInProgress.some(id => id === u.id)} onClick={() => {\n                           props.followThunkCreator(u.id)\n                        }}>Follow</button>}\n                        </div>\n                        </span>\n                <span>\n                        <span>\n                        <div>{u.name}</div>\n                        <div>{u.status}</div>\n                        </span>\n                        <span>\n                        <div>{\"u.location.country\"}</div>\n                        <div>{\"u.location.city\"}</div>\n                        </span>\n                        </span>\n\n            </div>)\n        }\n    </div>\n}\n\nexport default Users","/Users/brookly/Documents/my-first/my-app/src/Components/Common/Preloader.jsx",["168"],"import React from 'react'\nimport preloader from \"../Asets/images/Eclipse-1s-200px.svg\";\n\n\n\nconst Preloader=()=>{\n    return<div>\n        <img src={preloader}/>\n    </div>\n}\n\nexport default Preloader","/Users/brookly/Documents/my-first/my-app/src/Components/Profile/ProfileContainer.jsx",["169"],"/Users/brookly/Documents/my-first/my-app/src/redux/auth-reducer.js",["170","171"],"import {UsersAPI} from \"../api/api\";\nimport {setFollowingProgress, unfollow} from \"./Users-reducer\";\n\nconst SET_USERS_DATA = 'SET_USERS_DATA'\n\nlet initialState = {\n    id: null,\n    email: null,\n    login: null,\n    isAuth: false\n}\n\nconst authReducer = (state = initialState, action) => {\n\n    switch (action.type) {\n        case SET_USERS_DATA:\n            return {\n                ...state,\n                ...action.data,\n                isAuth: true\n            }\n        default:\n            return state\n    }\n}\n\nexport const setAuthUserData = (id, email, login) => ({type: SET_USERS_DATA, data: {id, email, login}})\n\nexport const authThunkCreator = () => {\n    return (dispatch) => {\n        UsersAPI.userLogin()\n            .then(data => {\n                if (data.resultCode === 0) {\n                    let {id, email, login} = data.data\n                    dispatch(setAuthUserDat